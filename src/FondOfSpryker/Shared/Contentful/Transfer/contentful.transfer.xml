<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ContentfulEntryRequest">
        <property name="id" type="string" />
        <property name="locale" type="string" />
    </transfer>

    <transfer name="ContentfulEntryResponse">
        <property name="id" type="string" />
        <property name="contentType" type="string" />
        <property name="fields" type="array" />
        <property name="errorMessage" type="string" />
        <property name="successful" type="bool" default="true" />
    </transfer>

    <transfer name="ContentfulNavigationUrlRequest">
        <property name="id" type="string" />
        <property name="locale" type="string" />
    </transfer>

    <transfer name="ContentfulNavigationUrlResponse">
        <property name="id" type="string" />
        <property name="title" type="string" />
        <property name="url" type="string" />
        <property name="errorMessage" type="string" />
        <property name="successful" type="bool" default="true" />
    </transfer>
</transfers>
